{
  "name": "@svvs/source",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "db:start1": "copy .env docker-compose\\.env && cd docker-compose && docker-compose up -d",
    "db:stop1": "cd docker-compose && docker-compose stop && del .env",
    "db:start": "docker-compose --env-file ./.env -f ./docker-compose/docker-compose.yml up -d",
    "db:stop": "docker-compose -f ./docker-compose/docker-compose.yml stop",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "backend-api:entitiesOrig": "node_modules/.bin/tsc --project apps/backend/api/tsconfig.entities.json",
    "backend-api:entities": "npx tsc --project apps/backend/api/tsconfig.entities.json",
    "db:migration:createOrig": "node_modules/.bin/ts-node node_modules/.bin/typeorm migration:create -n",
    "db:migration:create": "typeorm migration:create apps/backend/api/src/migrations/mUser",
    "db:migrations:runOrig": "node_modules/.bin/tsc --project apps/backend/api/tsconfig.migrations.json && node_modules/.bin/ts-node node_modules/.bin/typeorm migration:run -t=false",
    "db:migrations:run": "npx typeorm-ts-node-commonjs migration:run -d ./dataSource.ts -t=false",
    "db:migrations:revertOrig": "node_modules/.bin/ts-node node_modules/.bin/typeorm migration:revert",
    "db:migrations:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./dataSource.ts",
    "compodoc-backend-api": "mkdir docs-temp && copy README.md docs-temp\\ && copy apps\\backend\\api\\docs\\README.md .\\ && npx compodoc -p apps\\backend\\api\\tsconfig.app.json --theme vagrant -d docs\\api\\ && del README.md && copy docs-temp\\README.md .\\ && del docs-temp\\README.md && rmdir docs-temp",
    "compodoc-frontend-admin": "mkdir docs-temp && copy README.md docs-temp\\ && copy apps\\frontend\\admin\\docs\\README.md .\\ && npx compodoc -p apps\\frontend\\admin\\tsconfig.doc.json --theme vagrant -d docs\\admin\\ && del README.md && copy docs-temp\\README.md .\\ && del docs-temp\\README.md && rmdir docs-temp",
    "compodoc-frontend-client": "mkdir docs-temp && copy README.md docs-temp\\ && copy apps\\frontend\\client\\docs\\README.md .\\ && npx compodoc -p apps\\frontend\\client\\tsconfig.doc.json --theme vagrant -d docs\\client\\ && del README.md && copy docs-temp\\README.md .\\ && del docs-temp\\README.md && rmdir docs-temp",
    "compodoc": "npm run compodoc-backend-api && npm run compodoc-frontend-client && npm run compodoc-frontend-admin"
  },
  "private": true,
